{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    { "name": "esdoc-standard-plugin" },
    {
      "name": "esdoc-integrate-test-plugin",
      "option": {
        "source": "./specs/",
        "interfaces": ["describe", "it", "context", "suite", "test"],
        "includes": ["\\.spec\\.js$"]
      }
    }
  ]
}
