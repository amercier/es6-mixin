{
  "name": "es6-mixin",
  "version": "0.3.1",
  "description": "Minimalist mixin helper designed to be used with ES6 (ES2015) classes.",
  "license": "MIT",
  "repository": "amercier/es6-mixin",
  "author": "Alex Mercier <pro.alexandre.mercier@gmail.com> (http://amercier.com)",
  "contributors": [
    "Alex Mercier <pro.alexandre.mercier@gmail.com> (http://amercier.com)",
    "Andrew Noyess <noyesa@gmail.com> (https://github.com/noyesa)"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "transpile": "rm -rf dist && babel src -d dist",
    "prepublish": "npm run transpile",
    "doc": "rm -rf esdoc && esdoc -c esdoc.json",
    "lint": "eslint .",
    "mocha": "mocha specs --recursive --compilers js:babel-register",
    "start": "export NODE_ENV=development; npm run mocha -- --watch --growl",
    "test": "export NODE_ENV=test; npm run lint && rm -rf coverage && nyc npm run mocha"
  },
  "main": "dist/lib/index.js",
  "keywords": [
    "mixin",
    "mixins",
    "mix",
    "combine",
    "es6",
    "es2015",
    "es7",
    "es2016",
    "class",
    "protoype",
    "babel"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.7",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.2",
    "codecov": "^1.0.1",
    "esdoc": "^0.4.7",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^1.0.2",
    "eslint-plugin-import": "^1.10.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.0.0",
    "nyc": "^7.0.0",
    "object-assign": "^4.0.1",
    "sinon": "^1.17.3"
  },
  "nyc": {
    "exclude": "**",
    "reporter": [
      "lcovonly",
      "html",
      "text"
    ]
  }
}
